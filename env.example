# Ambiente da aplicação (development, testing, production)
APP_ENVIRONMENT=development

# Configurações do servidor
SERVER_HOST=127.0.0.1
SERVER_PORT=8080

# Configurações do banco de dados
DATABASE_URL=postgres://usuario:senha@localhost:5432/seu_banco
DATABASE_MAX_CONNECTIONS=5

# Configurações JWT
JWT_SECRET=sua_chave_secreta_muito_segura_com_pelo_menos_32_caracteres
JWT_EXPIRES_IN=86400  # 24 horas em segundos 